		</field>
		<field>
			<a>birth</a>
			<b>birth</b>
		</field>
		<field>
			<a>regdate</a>
			<b>regdate</b>
		</field>
	</mapping>
	<mapping date-format="yyyy-MM-dd">
		<class-a>com.gstore.dao.po.Favorate</class-a>
		<class-b>com.gstore.action.vo.FavorateVO</class-b>
		<field>
			<a>addDate</a>
			<b>addDate</b>
		</field>
	</mapping>
</mappings>