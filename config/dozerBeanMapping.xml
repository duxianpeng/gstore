			<b>hot</b>
		</field>
		<field>
			<a>addDate</a>
			<b>addDate</b>
		</field>
	</mapping>

	<mapping date-format="yyyy-MM-dd">
		<class-a>com.gstore.dao.po.User</class-a>
		<class-b>com.gstore.action.vo.UserVO</class-b>
		<field>
			<a>userName</a>
