package com.gstore.dao.po;

import java.util.Date;

public class Glasses {

	private int id;
	private String glassesName;
	private String glassesDesc;
	private int categoryId;
	private String categoryName;
	private String picture;
	private float price;
	private int hotNumber;
	private int antiUltraviolet;
	private Date addDate;
	private String intro;
	private int antiUVB;
	private String material;
	private String face;
	private int human;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getGlassesName() {
		return glassesName;
	}

	public void setGlassesName(String glassesName) {
		this.glassesName = glassesName;
	}

	public String getGlassesDesc() {
		return glassesDesc;
	}

	public void setGlassesDesc(String glassesDesc) {
		this.glassesDesc = glassesDesc;
	}

	public String getCategoryName() {
		return categoryName;
	}

	public void setCategoryName(String categoryName) {
		this.categoryName = categoryName;
	}

	public float getPrice() {
		return price;
	}

	public void setPrice(float price) {
		this.price = price;
	}

	public int getHotNumber() {
		return hotNumber;
	}

	public void setHotNumber(int hotNumber) {
		this.hotNumber = hotNumber;
	}

	public int getAntiUltraviolet() {
		return antiUltraviolet;
	}

	public void setAntiUltraviolet(int antiUltraviolet) {
		this.antiUltraviolet = antiUltraviolet;
	}

	public String getPicture() {
		return picture;
	}

	public void setPicture(String picture) {
		this.picture = picture;
	}

	public Date getAddDate() {
		return addDate;
	}

	public void setAddDate(Date addDate) {
		this.addDate = addDate;
	}

	public String getIntro() {
		return intro;
	}

	public void setIntro(String intro) {
		this.intro = intro;
	}

	public int getCategoryId() {
		return categoryId;
	}

	public void setCategoryId(int categoryId) {
		this.categoryId = categoryId;
	}

	public int getAntiUVB() {
		return antiUVB;
	}

	public void setAntiUVB(int antiUVB) {
		this.antiUVB = antiUVB;
	}

	public String getMaterial() {
		return material;
	}

	public void setMaterial(String material) {
		this.material = material;
	}

	public String getFace() {
		return face;
	}

	public void setFace(String face) {
		this.face = face;
	}

	public int getHuman() {
		return human;
	}

	public void setHuman(int human) {
		this.human = human;
	}

	@Override
	public String toString() {
		return "Glasses [id=" + id + ", glassesName=" + glassesName
				+ ", glassesDesc=" + glassesDesc + ", categoryName="
				+ categoryName + ", picture=" + picture + ", price=" + price
				+ ", hotNumber=" + hotNumber + ", antiUltraviolet="
				+ antiUltraviolet + ", addDate=" + addDate + "]";
	}

}
