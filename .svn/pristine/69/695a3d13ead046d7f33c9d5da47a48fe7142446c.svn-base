package com.gstore.action.vo;

import com.gstore.common.Constants;

public class SearchVO extends BaseVO {
	private String order="addDate";
	private Boolean isSearchInDesc;
	private String desc;
	private Integer categoryId;
	private Integer pfrom;
	private Integer pto;
	private String dfrom;
	private String dto;
	private Integer userId;
	private Integer glassesId;
	private Integer start = Constants.DEFAULT_START;
	private Integer limit = Constants.DEFAULT_PAGE_SIZE;

	public String getOrder() {
		return order;
	}

	public void setOrder(String order) {
		this.order = order;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

	public Boolean isSearchInDesc() {
		return isSearchInDesc;
	}

	public void setSearchInDesc(Boolean isSearchInDesc) {
		this.isSearchInDesc = isSearchInDesc;
	}

	public Integer getCategoryId() {
		return categoryId;
	}

	public void setCategoryId(Integer categoryId) {
		this.categoryId = categoryId;
	}

	public Integer getPfrom() {
		return pfrom;
	}

	public void setPfrom(Integer pfrom) {
		this.pfrom = pfrom;
	}

	public Integer getPto() {
		return pto;
	}

	public void setPto(Integer pto) {
		this.pto = pto;
	}

	public Boolean getIsSearchInDesc() {
		return isSearchInDesc;
	}

	public void setIsSearchInDesc(Boolean isSearchInDesc) {
		this.isSearchInDesc = isSearchInDesc;
	}

	public String getDfrom() {
		return dfrom;
	}

	public void setDfrom(String dfrom) {
		this.dfrom = dfrom;
	}

	public String getDto() {
		return dto;
	}

	public void setDto(String dto) {
		this.dto = dto;
	}

	public Integer getStart() {
		return start;
	}

	public void setStart(Integer start) {
		this.start = start;
	}

	public Integer getLimit() {
		return limit;
	}

	public void setLimit(Integer limit) {
		this.limit = limit;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public Integer getGlassesId() {
		return glassesId;
	}

	public void setGlassesId(Integer glassesId) {
		this.glassesId = glassesId;
	}

	@Override
	public String toString() {
		return "SearchVO [order=" + order + ", isSearchInDesc="
				+ isSearchInDesc + ", desc=" + desc + ", categoryId="
				+ categoryId + ", pfrom=" + pfrom + ", pto=" + pto + ", dfrom="
				+ dfrom + ", dto=" + dto + ", userId=" + userId
				+ ", glassesId=" + glassesId + ", start=" + start + ", limit="
				+ limit + "]";
	}



}
